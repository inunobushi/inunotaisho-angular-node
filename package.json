  "scripts": {
    "assets": "cp -R node_modules/font-awesome/fonts ./public/fonts && cp -R src/app/images ./public/images",
    "bundle": "npm run less && npm run pack",
    "bundle:main": "browserify src/main.js -d -t stringify -o public/dist/main.bundle.js -v -p [minifyify src/main.js.map --output public/dist/main.bundle.js.map]",
    "clean": "rimraf public/* && npm run assets",
    "less": "mkdirp public/dist && lessc ./src/app/styles/main.less > ./public/dist/app.css --clean-css=\"--s0 --advanced\"",
    "watch": "npm run watch:less",
    "watch:less": "",
    "refresh": "npm run less && node ./lib/app.js",
    "start": "concurrently -r \"npm run clean\" \"npm run bundle\" \"nodemon ./lib/app.js\"",
    "pack": "webpack --config webpack.config.dev.js --watch",
